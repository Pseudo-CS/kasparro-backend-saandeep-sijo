# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/etl_db
DB_HOST=localhost
DB_PORT=5432
DB_NAME=etl_db
DB_USER=postgres
DB_PASSWORD=postgres

# API Keys (DO NOT COMMIT ACTUAL KEYS)
# CoinPaprika API - Get your key from https://coinpaprika.com/api
API_KEY_SOURCE_1=your_coinpaprika_api_key_here
# CoinGecko API - Free tier, no key required (optional)
API_KEY_SOURCE_2=

# API URLs
# CoinPaprika API - Cryptocurrency data
API_URL_SOURCE_1=https://api.coinpaprika.com/v1/tickers
# CoinGecko API - Cryptocurrency market data
API_URL_SOURCE_2=https://api.coingecko.com/api/v3/coins/markets
# RSS Feed - Crypto news feed
RSS_FEED_URL=https://cointelegraph.com/rss

# CSV Paths
CSV_SOURCE_PATH=./data/sample_data.csv

# Application Settings
APP_NAME=ETL Backend Service
APP_VERSION=1.0.0
ENVIRONMENT=development
LOG_LEVEL=INFO

# API Server
API_HOST=0.0.0.0
API_PORT=8000

# ETL Settings
ETL_BATCH_SIZE=1000
ETL_RATE_LIMIT_CALLS=100
ETL_RATE_LIMIT_PERIOD=60
